@model IEnumerable<UserManagement.Models.UserActivity>

@{
    ViewData["Title"] = "UserActivityDetails";
}

<br />
<h1 style="color: #333;font-family:monospace"><strong>User Activity Details</strong></h1>
<hr /> 
<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Url)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
          
            <th>
                @Html.DisplayNameFor(model => model.ActivityDate)
            </th>
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Url)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
          
            <td>
                @Html.DisplayFor(modelItem => item.ActivityDate)
            </td>
           
        </tr>
}
    </tbody>
</table>
